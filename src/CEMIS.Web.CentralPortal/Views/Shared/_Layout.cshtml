<script type="text/javascript">
    $(document).ready(function () {

        @{
                if(TempData["Notification"] != null)
        {
            var notification = JsonConvert.DeserializeObject<Notification>(TempData["Notification"].ToString());
            if (notification.Type == NotificationsType.Success) {
                @: $.toast({ heading: '@notification.Title', text: '@notification.Message', showHideTransition: 'slide', icon: 'success', position: "top-right", });
            } else if (notification.Type == NotificationsType.Danger) {
                @: $.toast({ heading: '@notification.Title', text: '@notification.Message', showHideTransition: 'slide', icon: 'error', position: 'top-right', });
            } else if (notification.Type == NotificationsType.Info) {
                @: $.toast({ heading: '@notification.Title', text: '@notification.Message', showHideTransition: 'slide', icon: 'info', position: 'top-right', });
            } else if (notification.Type == NotificationsType.Warning) {
                @: $.toast({ heading: '@notification.Title', text: '@notification.Message', showHideTransition: 'slide', icon: 'warning', position: 'top-right', });
            }
        }
                }
            });
</script>
@RenderSection("scripts", required: false)
@RenderSection("JavaScript", required: false)