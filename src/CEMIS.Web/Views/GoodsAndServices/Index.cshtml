
@model IEnumerable<RevenueGoodandServiceHead>
@{
    ViewBag.Title = "Goods And Services";
    var i = 1;
}


<div class="page-title">
    <h3 class="breadcrumb-header">@ViewBag.Title</h3>
</div>
<div class="clearfix" style="text-align:right; padding: 20px">
    <a class="btn btn-success btn-md" href="@Url.Action("Create","GoodsAndServices")">Create</a>

</div>
<div id="main-wrapper">

    <div class="row">
        <div class="col-24">
            <div id="divModal">
            </div>
            <div class="row">
                <div class="col-md-24">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            &nbsp;
                        </div>
                        <div class="panel-body table-responsive">
                            <div class="table-responsive" style="">
                                <table id="example" class="display table table-active" style="width:100%;">
                                    <thead>
                                        <tr>
                                            <th>#</th>
                                            <th>Year</th>
                                            <th>Month</th>
                                            <th>MDA</th>
                                            <th>Returns On</th>
                                            <th>Fund Type</th>
                                            <th></th>
                                        </tr>
                                    </thead>

                                    <tbody>
                                        @foreach (var item in Model)
                                        {
                                            <tr class="odd gradeX">
                                                <td>@i</td>
                                                <td>@item.PeriodYear</td>
                                                <td>@item.PeriodMonth</td>
                                                <td>@item.MDA</td>
                                                <td>@item.ReturnsOn</td>
                                                <td>@item.FundType</td>
                                                <td>
                                                    <a class="btn btn-default" href="@Url.Action("edit","GoodsAndServices", new { Id = item.Id})">View</a>

                                                </td>
                                            </tr>
                                            i = i + 1;
                                        }
                                    </tbody>
                                </table>
                            </div>

                        </div>

                    </div>
                </div>
            </div>

        </div>
    </div>
</div>


@section JavaScript
    {


}





